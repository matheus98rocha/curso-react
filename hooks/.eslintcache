ect(() => {\n        if (number % 2 === 0) {\n            setStatus(\"Par\")\n        } else {\n            setStatus(\"Impar\")\n        }\n    }, [number])\n\n\n\n\n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n\n            <SectionTitle title=\"Exercício #01\" />\n\n            <div className=\"center\">\n                <div>\n                    <span className=\"text\">Fatorial:</span>\n                    <span className=\"text\">\n                        {fatorial === -1 ? \"Não existe\" : fatorial}\n                    </span>\n                </div>\n\n                <input type=\"number\" className=\"input\"\n                    value={number}\n                    onChange={e => setNumber(e.target.value)}\n                />\n            </div>\n\n            <SectionTitle title=\"Exercício #02\" />\n            <div className=\"center\">\n                <div>\n                    <span className=\"text\">Status:</span>\n                    <span className=\"text\">{status}</span>\n                </div>\n            </div>\n\n            <SectionTitle title=\"Exercício #03\" />\n            <div className=\"center\">\n                <span className=\"text\">Mudar titulo da página</span>\n\n                <div>\n\n                    <input type=\"text\"\n                        className=\"input\"\n                        value={title}\n                        onChange={e => setTitle(e.target.value)}\n                    />\n                </div>\n            </div>\n\n        </div>\n    )\n}\n\nexport default UseEffect\n",["76","77"],"/home/matheus98rocha/Área de Trabalho/cursoReact/hooks/src/views/examples/UseCallback.jsx",[],["78","79"],"/home/matheus98rocha/Área de Trabalho/cursoReact/hooks/src/views/examples/UseMemo.jsx",[],["80","81"],"/home/matheus98rocha/Área d